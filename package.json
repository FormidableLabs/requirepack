{
  "name": "requirepack",
  "version": "0.0.1",
  "description": "Webpack + RequireJS interoperable shared libraries",
  "main": "index.js",
  "scripts": {
    "lint-client": "eslint -c .eslintrc-client test/func/*/src",
    "lint-server": "eslint -c .eslintrc-server lib test/func/*/*.config*.js test/func/*/build.js ",
    "lint": "npm run lint-client && npm run lint-server",
    "test-build": "node test/func/*/build.js",
    "test-func": "echo 'TODO: Start FUNC TESTS'",
    "test-server": "http-server -p 3030 .",
    "test": "npm run lint && npm run test-build && npm run test-func"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/FormidableLabs/requirepack.git"
  },
  "author": "Ryan Roemer <ryan.roemer@formidablelabs.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/FormidableLabs/requirepack/issues"
  },
  "dependencies": {},
  "devDependencies": {
    "almond": "^0.3.1",
    "async": "^1.4.2",
    "eslint": "^1.5.1",
    "eslint-config-defaults": "^6.0.0",
    "eslint-plugin-filenames": "^0.1.2",
    "fs-extra": "^0.24.0",
    "http-server": "^0.8.5",
    "lodash": "^3.10.1",
    "requirejs": "^2.1.20",
    "webpack": "^1.12.2"
  }
